<div class="container">
  <div class="row">
    <div class="col-md-offline-4 col-md-4">
      <h1>Ajouter un client</h1>
    </div>
  </div>
  <div class="row">
    <div class="col-md-offline-4 col-md-4">
      <%= simple_form_for @client do |f| %>
      <%= f.input :nom %>
      <%= f.input :prenom %>
      <%= f.input :date_de_naissance %>
      <%= f.input :profession %>
      <%= f.input :adresse %>
      <%= f.input :portable %>
      <%= f.input :mail %>
      <%= f.input :categorie %>
      <%= f.input :nationalite %>
      <%= f.button :submit %>
      <% end %>
    </div>
  </div>
</div>



